{
  "name": "stardew-completionist",
  "productName": "Stardew Completionist",
  "version": "0.7.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Liamolucko/Stardew-Completionist.git"
  },
  "description": "An app designed to help people 100% Stardew Valley.",
  "main": "electron/dist/index.js",
  "scripts": {
    "dev": "sapper dev -p 4000",
    "export": "sapper export --legacy",
    "deploy": "npm run export && node scripts/deploy.js",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "electron": "tsc -p electron && electron-forge start",
    "package": "tsc -p electron && electron-forge package",
    "make": "tsc -p electron && electron-forge make",
    "publish": "tsc -p electron && electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "author": {
    "name": "Liamolucko",
    "email": "liampm32@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "stardew-completionist",
        "icon": "static/favicon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "stardew_completionist",
            "exe": "stardew-completionist.exe",
            "iconUrl": "https://raw.githubusercontent.com/Liamolucko/Stardew-Completionist/master/static/favicon.ico",
            "setupIcon": "static/favicon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "Liam Murphy"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@material/card": "^3.2.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^5.0.1",
    "@rollup/plugin-url": "^5.0.1",
    "@smui/button": "^1.0.0",
    "@smui/data-table": "^1.0.0",
    "@smui/dialog": "^1.0.0",
    "@smui/icon-button": "^1.0.0",
    "@smui/list": "^1.0.0",
    "@smui/textfield": "^1.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/gh-pages": "^3.0.0",
    "@types/node": "^12.12.54",
    "@types/polka": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "blob-polyfill": "^4.0.20200601",
    "electron": "9.1.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^3.1.0",
    "localforage": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.5",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.8",
    "sass": "^1.26.10",
    "svelte": "^3.24.1",
    "svelte-preprocess": "^4.1.1",
    "svelte-spinner": "^2.0.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "electron-serve": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "polka": "next",
    "sirv": "^0.4.0"
  }
}
