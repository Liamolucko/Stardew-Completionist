{
  "name": "stardew-completionist",
  "productName": "Stardew Completionist",
  "version": "0.7.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Liamolucko/Stardew-Completionist.git"
  },
  "description": "An app designed to help people 100% Stardew Valley.",
  "main": "electron/dist/index.js",
  "scripts": {
    "dev": "sapper dev -p 4000",
    "export": "sapper export --legacy",
    "deploy": "pnpm run export && gh-pages -d __sapper__/export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "electron": "tsc -p electron && electron-forge start",
    "package": "tsc -p electron && electron-forge package",
    "make": "tsc -p electron && electron-forge make",
    "publish": "tsc -p electron && electron-forge publish",
    "lint": "eslint --ext .ts .",
    "postinstall": "node ./scripts/fix-broken-symlinks.js"
  },
  "author": {
    "name": "Liamolucko",
    "email": "liampm32@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
